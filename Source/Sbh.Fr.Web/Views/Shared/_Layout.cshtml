                                    </div>*@
                                <div class="col-md-12">
                                    @RenderBody()

                                </div>
                                @*</div>*@
                            </div>
                        </div>
                    </div>
                    @Html.Partial("_QuickSidebar")
                </div>
            </div>
        </div>
        @Html.Partial("_Footer")
        <div class="row">
            <div class="modal fade" id="myModal" role="dialog">
            </div>
        </div>
    </div>

    @Html.Partial("_QuickNav")

    @Html.Partial("_QuickToggle")


